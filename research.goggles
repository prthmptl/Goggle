// =============================================================================
// CONTENT FARM AND SEO SPAM - HEAVY DOWNRANK OR DISCARD
// =============================================================================

// Known Content Farms
$discard,site=ehow.com
$discard,site=wikihow.com
$discard,site=answers.com
$discard,site=ask.com
$discard,site=yahoo.com/answers
$discard,site=quora.com/*/answer/*
$downrank=10,site=quora.com

// Low-Quality Tutorial Mills
$discard,site=tutorialspoint.com
$discard,site=javatpoint.com
$discard,site=geeksforgeeks.org
$discard,site=programiz.com
$discard,site=w3schools.com
$downrank=8,site=codecademy.com/forum-questions

// Spam Domains and Scrapers
$discard,site=*.blogspot.com
$discard,site=*.wordpress.com/*/feed
$discard,site=feedburner.com
$discard,site=medium.com/feed
$downrank=8,site=medium.com

// Pinterest and Social Media Spam
$discard,site=pinterest.com
$discard,site=pinterest.co.uk
$discard,site=pinterest.ca
$discard,site=pinterest.de
$discard,site=pinterest.fr
$discard,site=pinterest.it
$discard,site=pinterest.es
$discard,site=instagram.com
$discard,site=facebook.com
$discard,site=twitter.com/search

// PDF Spam and Document Mills
$downrank=5,*.pdf
$discard,site=scribd.com
$discard,site=slideshare.net
$discard,site=academia.edu
$discard,site=researchgate.net/publication

// =============================================================================
// SPECIFIC ANTI-SPAM PATTERNS
// =============================================================================

// Block common spam URL patterns
$discard,/how-to-*-in-*-easy-steps/
$discard,/ultimate-guide-to-*/
$discard,/complete-tutorial-*/
$discard,/*-tutorial-for-beginners/
$discard,/learn-*-in-*-minutes/
$discard,/top-*-tools-for-*/
$discard,/*-vs-*-which-is-better/
$discard,/best-*-for-*/
$discard,/*-cheat-sheet/
$downrank=5,/*/amp/

// Block low-quality comparison and listicle spam
$downrank=8,/top-10-*/
$downrank=8,/best-*/
$downrank=8,/*-comparison/
$downrank=8,/*-alternatives/
$downrank=6,/*-review/

// Boost quality technical content patterns
$boost=3,/documentation/
$boost=3,/docs/
$boost=3,/api/
$boost=3,/reference/
$boost=3,/guide/
$boost=3,/tutorial/
$boost=4,/blog/*/
$boost=2,/changelog/
$boost=2,/release-notes/

