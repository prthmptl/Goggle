$boost=7,site=joelonsoftware.com
$boost=7,site=codinghorror.com
$boost=7,site=techcrunch.com
$boost=7,site=arstechnica.com
$boost=7,site=lwn.net
$boost=7,site=ieee.org
$boost=7,site=acm.org
$boost=7,site=infoq.com
$boost=7,site=smashingmagazine.com
$boost=7,site=css-tricks.com
$boost=7,site=a11yproject.com
$boost=7,site=web.dev
$boost=7,site=webkit.org
$boost=7,site=v8.dev

// Quality Individual Developer Blogs
