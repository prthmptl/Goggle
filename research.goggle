// =============================================================================
// HIGH-QUALITY TECH SOURCES - BOOST HEAVILY
// =============================================================================

// Official Documentation and Standards
$boost=10,site=developer.mozilla.org
$boost=10,site=docs.python.org
$boost=10,site=docs.oracle.com
$boost=10,site=docs.microsoft.com
$boost=10,site=kubernetes.io
$boost=10,site=docker.com
$boost=10,site=nodejs.org
$boost=10,site=reactjs.org
$boost=10,site=vuejs.org
$boost=10,site=angular.io
$boost=10,site=golang.org
$boost=10,site=rust-lang.org
$boost=10,site=cppreference.com
$boost=10,site=w3.org
$boost=10,site=rfc-editor.org
$boost=10,site=ietf.org

// Quality Programming Communities
$boost=8,site=stackoverflow.com
$boost=8,site=github.com
$boost=8,site=gitlab.com
$boost=8,site=news.ycombinator.com
$boost=8,site=lobste.rs
